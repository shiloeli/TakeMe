        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.728" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="47dp"
        android:layout_height="34dp"
        android:text="מייל:"
